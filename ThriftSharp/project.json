{
  "version": "5.0.0",

  "title": "Thrift#",
  "authors": [ "Solal Pirelli" ],
  "description": "Attribute-based, IDL-less Apache Thrift client for .NET.",

  "packOptions": {
    "projectUrl": "https://github.com/SolalPirelli/ThriftSharp",
    "licenseUrl": "https://raw.githubusercontent.com/SolalPirelli/ThriftSharp/master/License.txt",
    "tags": [ "thrift" ]
  },

  "frameworks": {
    "netstandard1.3": {
      "dependencies": {
        "System.Collections": "4.0.11",
        "System.Collections.Concurrent": "4.0.12",
        "System.Linq": "4.1.0",
        "System.Linq.Expressions": "4.1.0",
        "System.Net.Http": "4.1.0",
        "System.Reflection": "4.1.0",
        "System.Reflection.DispatchProxy": "4.0.1",
        "System.Reflection.Extensions": "4.0.1",
        "System.Runtime.Extensions": "4.1.0",
        "System.Text.Encoding": "4.0.11",
        "System.Threading.Tasks": "4.0.11"
      }
    },

    "net451": {
      "dependencies": {
        "System.Net.Http": "4.0.0"
      },
      "buildOptions": {
        "define": [ "NET_FULL" ]
      }
    }
  }
}